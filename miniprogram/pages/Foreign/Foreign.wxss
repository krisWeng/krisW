/* pages/foreign/foreign.wxss */
.chinese{
  height: 450rpx;
  display: flex;
  justify-content: space-around;
  padding-top: 100rpx;
}
.chinese image{
  width: 300rpx;
  height: 300rpx;
}
.chinese text{
  line-height: 200rpx;
  margin-left: -100rpx;
}
/* 歌曲列表 */
.list{
  height: 100rpx;
  line-height: 100rpx;
  background: #fff;
  padding-left: 50rpx;
}
/* 列表内容 */
.ListCont{
  height: 130rpx;
  line-height: 130rpx;
  display: flex;
  background: #fff;
  border-top: 1px solid #f1f1f1;
}
.ListCont .num{
  width: 80rpx;
  text-align: center;
}
.listRight{
  width: 670rpx;
  display: flex;
  justify-content: space-between;
}
.listRight .songMsg{
  display: flex;
  flex-direction: column;
  margin-left: 20rpx;
  margin-top: 20rpx;
}
.listRight .songMsg .song{
  height: 50rpx;
  line-height: 50rpx;
  font-size: 15px;
}
.listRight .songMsg .singer{
  height: 50rpx;
  line-height: 50rpx;
  font-size: 12px;
  color: #888888;
}
.listRight .play{
  width: 60rpx;
  height: 60rpx;
  margin-right: 30rpx;
  align-self: center;
}
/* 背景图 */
.after{
  width: 750rpx;
  height: 450rpx;
  position: absolute;
  opacity: calc(0.5);
	filter: blur(1.125rem);
  background: url(data:image/jpeg;base64,)
}