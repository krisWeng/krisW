/* pages/self/self.wxss */
/* 用户信息 */
.selfTop{
  width: 750rpx;
  height: 350rpx;
}
.usermsg{
  display: flex;
  justify-content: space-between;
  padding: 20rpx 30rpx 0 25rpx;
}
.userLeft{
   display: flex;
}
.usermsg .userLeft image{
  width: 150rpx;
  height: 150rpx;
  border-radius: 99px;
}
.usermsg .userLeft .NameGrade{
  display: flex;
  flex-direction: column;
  margin-left: 15px;
}
.usermsg .userLeft .NameGrade .userName{
  font-size: 18px;
  font-weight: bolder;
}
.usermsg .userLeft .NameGrade .userGrade{
  width: 100rpx;
  font-size: 12px;
  text-align: center;
  border-radius: 20px;
  margin-top: 10px;
  background: #e6e2df;
}
.usermsg .SignIn{
  width: 150rpx;
  height: 63rpx;
  text-align: center;
  font-size: 13px;
  line-height: 60rpx;
  border-radius: 20px;
  color: #fff;
  background: #f32c15;
  margin-top: 30rpx;
}
/* 动态 关注 粉丝 */
.means{
  display: flex;
  justify-content: space-around;
  margin-top: 40rpx;
}
.means .trends{
  width: 187rpx;
  display: flex;
  flex-direction: column;
  text-align: center;
  border-right: 1px solid #d2cecf; 
}
.means .trends:nth-of-type(4){
  border-right: 0;
}
.means .trends:nth-of-type(4) navigator{
  display: flex;
  flex-direction: column;
}
.means .trends .meansNum{
  font-size: 18px;
}
.means .trends .meansName{
  font-size: 12px;
  color: #aba7a8;
}
.means .trends image{
  width: 60rpx;
  height: 60rpx;
  align-self: center;
}
/* 第二部分 */
.selfTwo{
  background: #fff;
  display: flex;
  flex-direction: row;
  padding-top: 15px;
  padding-bottom: 10px;
}
.selfTwo .left{  
  width: 150rpx;
  display: flex;
  flex-direction: column;
  padding-left: 5px;
}
.selfTwo .left .leftTop{
  font-size: 28px;
}
.selfTwo .right{
  width: 570rpx;
  font-size: 14px;
}
/* 退出登录 */
button{
  width: 750rpx;
  background: #fff;
  margin-top: 30rpx;
  color: red;
}

/* 背景图 */
.after{
  width: 750rpx;
  height: 350rpx;
  position: absolute;
  z-index: -99;
  opacity: calc(0.1);
	filter: blur(1.5rem);
  background:url(data:image/jpeg;base64,);
}